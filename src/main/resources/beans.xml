<?xml version="1.0" encoding="UTF-8" ?>
<beans>
    <bean id="baseservice" class="org.springframework.demo6.service.BaseService"/>

    <bean id="aservice" class="org.springframework.demo6.service.AServiceImpl">
        <constructor-arg type="String" name="name" value="abc"/>
        <constructor-arg type="int" name="level" value="3"/>
        <property type="String" name="property1" value="Someone says"/>
        <property type="String" name="property2" value="Hello World!"/>
        <property type="org.springframework.demo6.service.BaseService" name="ref1" ref="baseservice"/>
    </bean>


</beans>